{
    "name": "avkluchko/api-platform-extensions",
    "description": "Useful extensions for Api Platform",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "api-platform/core": "^2.5",
        "doctrine/orm": "^2.6",
        "symfony/security-core": "^4.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\ApiPlatformExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\ApiPlatformExtensions\\Tests\\": "tests/"
        }
    }
}
